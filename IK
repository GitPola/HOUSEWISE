function geostate() {
    var t = {},
        a = !1,
        o = !1,
        n = function () {},
        i = function () {},
        r = function () {},
        s = { kde: [0, 0, 0, ""], mapa: [], vrstvy: [], info: [0, 0] };
    function u(e, t) {
        return -1 != e.indexOf(t);
    }
    function l(e) {
        var t,
            a = [];
        for (t in e) {
            var n,
                o = e[t];
            if (null != o && "function" != typeof o) {
                if ("object" == typeof o && o.constructor == Array) {
                    for (var i, r = [], s = 0, l = o.length; s < l; s++) (i = o[s]), r.push(encodeURIComponent(null == i ? "" : i));
                    n = r.join(",");
                } else n = encodeURIComponent(o);
                a.push(encodeURIComponent(t) + "=" + n);
            }
        }
        return a.join("&");
    }
    function c(e) {
        for (
            var t,
                a,
                n = "",
                o =
                    (u((e = e || window.location.href), "#")
                        ? ((t = e.indexOf("#") + 1), (a = e.length), (n = e.substring(t, a)))
                        : u(e, "?") && ((t = e.indexOf("?") + 1), (a = u(e, "#") ? e.indexOf("#") : e.length), (n = e.substring(t, a))),
                    {}),
                i = n.split(/[&;]/),
                r = 0,
                s = i.length;
            r < s;
            ++r
        ) {
            var l,
                c = i[r].split("=");
            c[0] && ((l = decodeURIComponent(c[0])), (c = c[1] || ""), 1 == (c = decodeURIComponent(c.replace(/\+/g, " ")).split(",")).length && (c = c[0]), (o[l] = c));
        }
        return o;
    }
    function p(e, t) {
        return Math.floor(e * t) / t;
    }
    function d(e) {
        (a = !0), (window.location.hash = e);
    }
    function m(e) {
        var t = c(window.location.href);
        n(t.kde), t && null != t.mapa && null != t.vrstvy && ((t.mapa = "string" == typeof t.mapa ? [t.mapa] : t.mapa), (t.vrstvy = "string" == typeof t.vrstvy ? [t.vrstvy] : t.vrstvy)), i(t.mapa, t.vrstvy), t && r(t.info);
    }
    return (
        window.addEventListener("hashchange", function (e) {
            a || ((o = !0), m(window.location.href)), (a = !1);
        }),
        (t.storePosition = function (e, t, a, n) {
            (o && !n) || (((n = c(window.location.href)).kde = [p(e, 1e5), p(t, 1e5), p(a, 10)]), JSON.stringify(n.kde) != JSON.stringify(s.kde) && ((s.kde = n.kde), d("#" + l(n)))), (o = !1);
        }),
        (t.storeLayers = function (e, t) {
            var a = c(window.location.href);
            (a.mapa = e), (a.vrstvy = t), (JSON.stringify(a.mapa) == JSON.stringify(s.mapa) && JSON.stringify(a.vrstvy) == JSON.stringify(s.vrstvy)) || ((s.mapa = e), (s.vrstvy = t), d("#" + l(a)));
        }),
        (t.storeInfo = function (e, t) {
            var a = c(window.location.href);
            (a.info = [p(e, 1e5), p(t, 1e5)]), JSON.stringify(a.info) != JSON.stringify(s.info) && ((s.info = a.info), d("#" + l(a)));
        }),
        (t.removeInfo = function () {
            var e = c(window.location.href);
            delete e.info, d("#" + l(e));
        }),
        (t.readStateFromUrl = m),
        (t.onPositionChanged = function (e) {
            return arguments.length ? ((n = e), t) : n;
        }),
        (t.onLayersChanged = function (e) {
            return arguments.length ? ((i = e), t) : i;
        }),
        (t.onInfoChanged = function (e) {
            return arguments.length ? ((r = e), t) : r;
        }),
        t
    );
}
function geomeasureUI(t, e) {
    var a,
        n = {},
        o = document.getElementById(e + "Btn"),
        i = document.getElementById(e + "AreaBtn"),
        e = geoutils(),
        r = function () {},
        s = function () {};
    function l() {
        c(), u();
    }
    function c() {
        (o.style.color = ""),
            (o.selected = !1),
            (i.style.color = ""),
            (i.selected = !1),
            t.off("editable:drawing:end"),
            t.off("editable:vertex:new"),
            t.off("editable:drawing:move"),
            t.editTools && t.editTools.stopDrawing(),
            a && (a.disableEdit(), t.removeLayer(a), (a = null)),
            t.doubleClickZoom.enable(),
            s();
    }
    function u() {
        t.on("editable:vertex:new", p), t.on("editable:drawing:move", d), t.on("editable:drawing:end", m), t.doubleClickZoom.disable(), r();
    }
    function p(e) {
        0 == e.vertex.getIndex() && 1 == e.layer._latlngs.length && (a && (a.disableEdit(), t.removeLayer(a), (a = null)), (a = e.layer)), e.layer.showMeasurements(), e.layer.updateMeasurements();
    }
    function d(e) {
        t.editTools.forwardLineGuide.showMeasurements(), e.layer && (t.editTools.forwardLineGuide.updateMeasurements({ prevLength: e.layer.getTotalLength() }), e.layer.updateMeasurements());
    }
    function m(e) {
        o.selected ? t.editTools.startPolyline() : t.editTools.startPolygon();
    }
    return (
        (o.selected = !1),
        o.addEventListener("click", function (e) {
            0 == o.selected ? (l(), t.editTools.startPolyline(), (o.style.color = "rgb(237, 165, 113)"), (o.selected = !0)) : c();
        }),
        (i.selected = !1),
        i.addEventListener("click", function (e) {
            0 == i.selected ? (l(), t.editTools.startPolygon(), (i.style.color = "rgb(237, 165, 113)"), (i.selected = !0)) : c();
        }),
        0 == e.isMobile() &&
            e.loadScript("dist/Leaflet.Editable.min.js", function () {
                (o.style.display = "block"), (i.style.display = "block"), (t.editTools = new L.Editable(t, {}));
            }),
        (n.onStart = function (e) {
            return arguments.length ? ((r = e), n) : r;
        }),
        (n.onStop = function (e) {
            return arguments.length ? ((s = e), n) : s;
        }),
        (n.start = u),
        (n.stop = c),
        (n.started = !1),
        n
    );
}
function ikalinks(e, t) {
    function a(e) {
        return document.getElementById(e);
    }
    var n = a("rightPanel"),
        o = a("mynemo"),
        i = a("ceszem"),
        r = o ? o.getAttribute("href") : null,
        s = document.querySelector("#mynemo h2 #mn_next"),
        l = s ? s.innerText : null,
        c = null != i ? i.getAttribute("href") : null,
        u = geoutils(),
        p = a("rightPanel_hide"),
        d = a("rightPanel_show"),
        m = "showRightPanelUI",
        h = {},
        v = function () {};
    function y(e) {
        (n.style.display = "none"), u.setCookie(m, "no", 365);
    }
    function w() {
        v(), (n.style.display = "block"), u.setCookie(m, "yes", 365);
    }
    function g(e, t) {
        return Math.floor(e * t) / t;
    }
    return (
        p.addEventListener("click", function (e) {
            y();
        }),
        d.addEventListener("click", function (e) {
            w();
        }),
        ("no" == u.getCookie(m) ? y : w)(),
        (h.updateParcelInfo = function (e) {
            u.log(":::received Parcel info:::" + JSON.stringify(e)), o && ((o.href = o.href + "&idp=" + e.cpId + "&parcela=" + e.cpTag), (s.innerHTML = l + ": " + e.cpTag));
        }),
        (h.updateBuildingInfo = function (e) {
            u.log(":::received Building info:::" + JSON.stringify(e));
        }),
        (h.updateKuInfo = function (e, t) {
            u.log(":::received Ku info:::" + JSON.stringify(e)), o && ((o.href = o.href + "&ku=" + e.kuTag + "&kuid=" + e.kuId), u.log("ku = " + e.kuTag));
        }),
        (h.updateAdminInfo = function (e, t) {
            u.log(":::received Admin info:::" + JSON.stringify(e));
        }),
        (h.updateLocationInfo = function (e, t, a) {
            void 0 !== i && null != i && ((i.href = c + "?lat=" + e + "&lng=" + t + "&zoom=" + a + "&from=ikatastr"), u.log("Lat : " + e + " Lon: " + t + " Zoom: " + a));
        }),
        (h.newQuery = function (e) {
            u.log(":::::::::::::::::::::::::::::::::::::"), u.log("::: NEW QUERY :::" + JSON.stringify(e)), o && ((o.href = r + "&ilat=" + g(e.lat, 1e5) + "&ilng=" + g(e.lng, 1e5)), (s.innerText = l));
        }),
        (h.onShow = function (e) {
            return arguments.length ? ((v = e), h) : v;
        }),
        (h.isVisible = function () {
            return "none" != n.style.display;
        }),
        (h.hide = y),
        h
    );
}
function appM() {
    var a,
        n,
        e,
        o,
        i,
        t = {},
