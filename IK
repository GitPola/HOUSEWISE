    }
    function f(e, t, a) {
        return (e = e && e.getAttribute(t)) ? e.substring(e.lastIndexOf(a) + a.length) : null;
    }
    function k(e, t, a, n) {
        return f(e.getElementsByTagName(t)[0], a, n);
    }
    function z(e) {
        var t = [];
        for (c = 0; c < e.length; c++) {
            var a = e[c].textContent;
            if (a) {
                var n = a.split(" "),
                    o = [];
                if (0 == c) for (var i = 0; i <= n.length / 2 - 1; i++) o.push([n[2 * i + 0], n[2 * i + 1]]);
                else for (i = n.length / 2 - 1; 0 <= i; i--) o.push([n[2 * i + 0], n[2 * i + 1]]);
            }
            t.push(o);
        }
        return t;
    }
    function b(e, t, a) {
        if (!e || 0 == e.length) return !1;
        if (!K(e[0], t, a)) return null;
