        .onPositionChanged(function (e) {
            null != e ? m.setView([e[0], e[1]], e[2]) : y || (console.log("...setting default view"), m.setView([49.747, 15.7673], 8));
        })
        .onLayersChanged(function (e, t) {
            w.setLayers(e, t);
        })
