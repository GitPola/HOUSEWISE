    function h() {
        var e = m.getCenter();
        e.lat != c.lat && e.lng != c.lng && m._zoom != c.zoom && (v.storePosition(e.lat, e.lng, m._zoom), r.setCookie(s, window.location.hash, 365)), i.updateLocationInfo(e.lat, e.lng, m.getZoom());
    }
