    }
    function K(e, t, a) {
        for (var n = 0, o = e[e.length - 1][0], i = e[e.length - 1][1], r = 0; r < e.length; r++) {
            var s = e[r][0],
                l = e[r][1];
            i <= a ? a < l && 0 < (s - o) * (a - i) - (t - o) * (l - i) && n++ : l <= a && (s - o) * (a - i) - (t - o) * (l - i) < 0 && n--, (o = s), (i = l);
        }
        return 0 !== n;
    }
    return (
        (a.queryParcelBuilding = function (e) {
            (e.gfi = "I=" + e.pX.toFixed(0) + "&J=" + e.pY.toFixed(0) + "&BBOX=" + e.bbox + "&WIDTH=" + e.width + "&HEIGHT=" + e.height), y(e), w(e), 0 == u.isMobile() && g(e);
