        s = "lastState",
        l = document.getElementById("cookies_agreeBtn"),
        c = { lat: 49.747, lng: 15.7673, zoom: 8 },
        u = document.getElementById("plusBtn"),
        p = document.getElementById("minusBtn"),
        d =
            (r.getCookie("agreeM") ||
                ((document.getElementById("agreement").style.display = "block"),
