                                    z();
                                })
                                .onLoaded(function () {})),
                            o.getPanorama();
                    }),
                    setTimeout(
                        function () {
                            r.loadScript("https://www.googletagmanager.com/gtag/js?id=UA-194652-4", function () {
                                r.log("Analytics loaded");
                            });
                        },
                        1 == r.isMobile() ? 1e4 : 0
                    ),
                    m.on("moveend", function () {
                        h();
                    }),
                    m.on("click", d.clickReceiver),
                    m.on("dblclick", d.dblClickReceiver);
            }),
            r.singleClickHandler().onSingleClick(function (e) {
                (r.isMobile() && 1 == k([w, n, f])) || o.onClick(e);
            }));
