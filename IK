                    r.setCookie("agreeM", "yes", 365), (document.getElementById("agreement").style.display = "none");
                })),
            0 == r.isMobile() && (l = r.getCookie(s)) && -1 == window.location.href.indexOf("#") && (window.location.hash = l),
            (window.onload = function (e) {
                -1 != window.location.href.indexOf("#") || y ? v.readStateFromUrl(window.location.href) : m.setView([c.lat, c.lng], c.zoom);
                {
                    var t;
                    null != i &&
                        ((t = m.getCenter()).lat != c.lat && t.lng != c.lng && m._zoom != c.zoom && (v.storePosition(t.lat, t.lng, m._zoom), r.setCookie(s, window.location.hash, 365)), i.updateLocationInfo(t.lat, t.lng, m.getZoom()));
                }
