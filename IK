            e && e[0] && e[1] ? o.queryOnLatLng(e[0], e[1]) : o.cleanup();
        });
    m.on(
        "zoomend zoomlevelschange",
        function () {
            (u.style.color = m._zoom == m.getMaxZoom() ? "grey" : "white"), (p.style.color = m._zoom == m.getMinZoom() ? "grey" : "white");
        },
        this
    ),
