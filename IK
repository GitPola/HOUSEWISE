function JTSK() {
    function e() {
        (this.distPoints = function (e, t, a, n) {
            e = this.hypot(e - a, t - n);
            return e < this.EPS ? 0 : e;
        }),
            (this.transformCoords = function (e, t, a) {
                var n = ((4.99821 / 3600) * Math.PI) / 180,
                    o = ((1.58676 / 3600) * Math.PI) / 180,
                    i = ((5.2611 / 3600) * Math.PI) / 180,
                    r = -3543e-9;
                return [(1 + r) * (+e + i * t - o * a) - 570.69, (1 + r) * (-i * e + t + n * a) - 85.69, (1 + r) * (o * e - n * t + a) - 462.84];
            }),
            (this.deg2rad = function (e) {
                return (e / 180) * Math.PI;
            }),
            (this.rad2deg = function (e) {
                return (e / Math.PI) * 180;
            }),
            (this.hypot = function (e, t) {
                return Math.sqrt(e * e + t * t) || 0;
            });
    }
    return (
        (e.prototype.EPS = 1e-4),
        (e.prototype.JTSKtoWGS84 = function (e, t) {
            if (!e || !t) return { lat: 0, lon: 0 };
            for (
                var a, n, o, i, r = 5, s = 49, l = 14, c = 0;
                (a = (i = this.WGS84toJTSK(s - r, l - r)).x && i.y ? this.distPoints(i.x, i.y, e, t) : 1e32),
                    (n = (i = this.WGS84toJTSK(s - r, l + r)).x && i.y ? this.distPoints(i.x, i.y, e, t) : 1e32),
                    (o = (i = this.WGS84toJTSK(s + r, l - r)).x && i.y ? this.distPoints(i.x, i.y, e, t) : 1e32),
                    (i = (i = this.WGS84toJTSK(s + r, l + r)).x && i.y ? this.distPoints(i.x, i.y, e, t) : 1e32),
                    a <= n && a <= o && a <= i && ((s -= r / 2), (l -= r / 2)),
                    n <= a && n <= o && n <= i && ((s -= r / 2), (l += r / 2)),
                    o <= a && o <= n && o <= i && ((s += r / 2), (l -= r / 2)),
                    i <= a && i <= n && i <= o && ((s += r / 2), (l += r / 2)),
                    (c += 4),
                    !((r *= 0.55) < 1e-5 || 1e3 < c);

            );
            return { lat: s, lon: l };
        }),
        (e.prototype.WGS84toJTSK = function (e, t) {
            return e < 40 || 60 < e || t < 5 || 25 < t ? { x: 0, y: 0 } : ((e = this.WGS84toBessel(e, t)), this.BesseltoJTSK(e[0], e[1]));
        }),
        (e.prototype.WGS84toBessel = function (e, t, a) {
            var n = this.deg2rad(e),
                o = this.deg2rad(t),
                n = this.BLHToGeoCoords(n, o, a || 0),
                o = this.transformCoords(n[0], n[1], n[2]),
                a = this.geoCoordsToBLH(o[0], o[1], o[2]);
            return [(e = this.rad2deg(a[0])), (t = this.rad2deg(a[1]))];
        }),
        (e.prototype.BesseltoJTSK = function (e, t) {
            var a = 0.081696831215303,
                n = 0.97992470462083,
                o = 0.420215144586493,
                i = 0.907424504992097,
                r = 1.000597498371542,
                e = this.deg2rad(e),
                t = this.deg2rad(t),
                e = Math.sin(e),
                s = (1 - a * e) / (1 + a * e),
                s = (Math.pow(1 + e, 2) / (1 - Math.pow(e, 2))) * Math.exp(a * Math.log(s)),
                e = ((s = 1.00685001861538 * Math.exp(r * Math.log(s))) - 1) / (1 + s),
                a = Math.sqrt(1 - e * e),
                s = r * t,
                r = Math.sin(s),
                t = Math.cos(s),
                s = o * t - i * r,
                e = 0.863499969506341 * e + 0.504348889819882 * a * (i * t + o * r),
                i = Math.sqrt(1 - e * e),
                t = (s * a) / i,
                o = Math.sqrt(1 - t * t),
                r = n * Math.atan(t / o),
                s = 12310230.12797036 * Math.exp(-n * Math.log((1 + e) / i));
            return { x: s * Math.cos(r), y: s * Math.sin(r) };
        }),
        (e.prototype.BLHToGeoCoords = function (e, t, a) {
            var n = 1 - Math.pow(0.9966471893352525, 2),
                o = 6378137 / Math.sqrt(1 - n * Math.pow(Math.sin(e), 2));
            return [(o + a) * Math.cos(e) * Math.cos(t), (o + a) * Math.cos(e) * Math.sin(t), ((1 - n) * o + a) * Math.sin(e)];
        }),
        (e.prototype.geoCoordsToBLH = function (e, t, a) {
            var n = 6377397.15508,
                o = 299.152812853,
                i = o / (o - 1),
                r = Math.sqrt(Math.pow(e, 2) + Math.pow(t, 2)),
                o = 1 - Math.pow(1 - 1 / o, 2),
                s = Math.atan((a * i) / r),
                l = Math.sin(s),
                s = Math.cos(s),
                a = (a + o * i * n * Math.pow(l, 3)) / (r - o * n * Math.pow(s, 3)),
                i = Math.atan(a),
                l = Math.sqrt(1 + a * a) * (r - n / Math.sqrt(1 + (1 - o) * a * a));
            return [i, 2 * Math.atan(t / (r + e)), l];
        }),
        new e()
    );
}
function geoutils() {
    var e = {},
        t = null;
    return (
        (e.setCookie = function (e, t, a) {
            var n = new Date(),
                a = (n.setTime(n.getTime() + 24 * a * 60 * 60 * 1e3), "expires=" + n.toUTCString());
            document.cookie = e + "=" + t + ";" + a + ";path=/";
        }),
        (e.getCookie = function (e) {
            for (var t = e + "=", a = document.cookie.split(";"), n = 0; n < a.length; n++) {
                for (var o = a[n]; " " == o.charAt(0); ) o = o.substring(1);
                if (0 == o.indexOf(t)) return o.substring(t.length, o.length);
            }
            return "";
        }),
        (e.log = function (e) {
            (-1 == window.location.hostname.indexOf("localhost") && -1 == window.location.hash.indexOf("log=1")) || console.log(e);
        }),
        (e.loadScript = function (e, t) {
            var a = document.createElement("script");
            (a.charset = "utf-8"),
                (a.onload = t),
                a.attachEvent &&
                    a.attachEvent("onreadystatechange", function (e) {
                        ("loaded" != e.srcElement.readyState && "complete" != e.srcElement.readyState) || t();
                    }),
                document.head.appendChild(a),
                (a.src = e),
                (a.type = "text/javascript");
        }),
        (e.isMobile = function () {
            var e;
            return (
                null == t &&
                    ((e = navigator.userAgent.toLowerCase()),
                    (t =
                        -1 < e.indexOf("iphone") ||
                        -1 < e.indexOf("ipod") ||
                        -1 < e.indexOf("ipad") ||
                        -1 < e.indexOf("android") ||
                        -1 < e.indexOf("windows phone os") ||
                        -1 < e.indexOf("blackberry") ||
                        -1 < e.indexOf("windows ce") ||
                        -1 < e.indexOf("series60") ||
                        -1 < e.indexOf("symbian") ||
                        -1 < e.indexOf("palm") ||
                        -1 < e.indexOf("IEMobile") ||
                        -1 < e.indexOf("Opera Mini"))),
                t
            );
        }),
        (e.isAndroid = function () {
            return -1 < navigator.userAgent.toLowerCase().indexOf("android");
        }),
        (e.isiOS = function () {
            var e = navigator.userAgent.toLowerCase();
            return -1 < e.indexOf("iphone") || -1 < e.indexOf("ipod") || -1 < e.indexOf("ipad");
        }),
        (e.formatArea = function (e) {
            var t = 1e6 < e ? ((e /= 1e6), "kmÂ˛") : "mÂ˛";
            return e < 100 ? e.toFixed(1) + " " + t : Math.round(e) + " " + t;
        }),
        (e.singleClickHandler = function () {
            var t,
                a = {},
                n = 0,
                o = !1,
                i = function () {};
            return (
                (a.onSingleClick = function (e) {
                    return arguments.length ? ((i = e), a) : i;
                }),
                (a.clickReceiver = function (e) {
                    return (
                        1 == ++n
                            ? ((o = !0),
                              (t = setTimeout(function () {
                                  1 == o && (i(e), (n = 0));
                              }, 250)))
                            : (clearTimeout(t), (n = 0), (o = !1)),
                        a
                    );
                }),
                (a.dblClickReceiver = function (e) {
                    o = !1;
                }),
                a
            );
        }),
        e
    );
}
